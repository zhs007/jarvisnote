#jarvis note config

# bindaddr - jarvis note bind address
bindaddr: ':7720'
# servaddr - jarvis note server address
servaddr: '127.0.0.1:7720'
# jarvisnodecfg - jarvis node config
jarvisnodecfg: './cfg/jarvisnode.yaml'

# ankadb - ankadb config
ankadb:
  # dbpath - dbpath
  dbpath: './dat'
  # ankadb engine
  engine: 'leveldb'